<IfDefine !IS_SAMPLE_LOADED>
  Define IS_SAMPLE_LOADED
  WSGIDaemonProcess sample python-home=/opt/bitnami/python python-path=/opt/bitnami/projects/sample processes=2 threads=15
</IfDefine>
<VirtualHost 127.0.0.1:80 _default_:80>
  ServerAlias *
  WSGIProcessGroup sample
  Alias /robots.txt /opt/bitnami/projects/sample/static/robots.txt
  Alias /favicon.ico /opt/bitnami/projects/sample/static/favicon.ico
  Alias /static/ /opt/bitnami/projects/sample/static/
  <Directory /opt/bitnami/projects/sample/static>
    Require all granted
  </Directory>
  Alias /media/ /opt/bitnami/projects/sample/media/
  <Directory /opt/bitnami/projects/sample/media>
    Require all granted
  </Directory>
  WSGIScriptAlias / /opt/bitnami/projects/sample/sample/wsgi.py
  <Directory /opt/bitnami/projects/sample/sample>
    <Files wsgi.py>
      Require all granted
    </Files>
  </Directory>
</VirtualHost>
